<!DOCTYPE html>

<title>YearDetail</title>

{% block body %}
    <h1>YearDetail</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ year_detail.id }}</td>
            </tr>
            <tr>
                <th>Moyennes</th>
                <td>{{ year_detail.moyennes ? year_detail.moyennes|join(', ') : '' }}</td>
            </tr>
            <tr>
                <th>Eng</th>
                <td>{{ year_detail.eng }}</td>
            </tr>
            <tr>
                <th>Fr</th>
                <td>{{ year_detail.fr }}</td>
            </tr>
            <tr>
                <th>AcademicYear</th>
                <td>{{ year_detail.academicYear }}</td>
            </tr>
            <tr>
                <th>Authentic</th>
                <td>{{ year_detail.authentic ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_year_detail_index') }}">back to list</a>

    <a href="{{ path('app_year_detail_edit', {'id': year_detail.id}) }}">edit</a>

    {{ include('year_detail/_delete_form.html.twig') }}
{% endblock %}
